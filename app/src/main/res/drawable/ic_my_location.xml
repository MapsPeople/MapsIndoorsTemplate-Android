<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape
			android:shape="oval">
			<solid android:color="#7f2196F3"/>
			<size
				android:width="24dp"
				android:height="24dp"/>
		</shape>
	</item>
	<item
		android:bottom="2dp"
		android:left="2dp"
		android:top="2dp"
		android:right="2dp"
		>
		<shape
			android:shape="oval">
			<solid android:color="#2196F3"/>
			<stroke
				android:width="2dp"
				android:color="#FFF"/>
		</shape>
	</item>
</layer-list>